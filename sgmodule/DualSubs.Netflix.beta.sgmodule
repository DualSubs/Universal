[Script]
DualSubs.Netflix.Manifest.json = type=http-response, pattern=^https?:\/\/www\.netflix\.com\/playapi\/cadmium\/manifest\/(.*)(\?.*), requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.Netflix.Manifest.beta.js
DualSubs.Netflix.PBO_Manifest.json = type=http-response, pattern=^https?:\/\/(ios|win10)\.prod\.(ftl|http1)\.netflix\.com\/nq(\/msl|msl_v1)?(\/cadmium)?(\/iosplatform)?\/pbo_manifest\/~1\.0\.0\/router, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.Netflix.Manifest.beta.js
DualSubs.Netflix.SUB.WebVTT = type=http-response, pattern=https?:\/\/.+\.nflxvideo.net\/(\?.*dualsubs=\w+)$, requires-body=1, max-size=0, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.SUB.WebVTT.beta.js
DualSubs.Netflix.SUB.TTML = type=http-response, pattern=https?:\/\/.+\.nflxvideo.net\/(\?.*dualsubs=\w+)$, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.SUB.TTML.beta.js

